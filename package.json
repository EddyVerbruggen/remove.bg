  },
  "homepage": "https://github.com/remove-bg/remove.bg#readme",
  "dependencies": {
    "request": "^2.88.0",
    "unirest": "~0.5.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/request": "^2.48.1",
    "typescript": "~3.2.4"
  }
